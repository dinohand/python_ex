    "def add_mango(function):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        # print(\"Adding mango to the function call\")\n",
    "        return \"Mango-\" +  function(*args, **kwargs)\n",
    "    return wrapper\n",
    "\n",
    "# @add_mango\n",
    "def make_smoothie(fruit):\n",
    "    return f\"{fruit} smoothie\"\n",
    "\n",
    "make_smoothie(\"banana\")"
   ]
  }
