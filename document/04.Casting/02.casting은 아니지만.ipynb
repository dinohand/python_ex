    "# round, ceil, floor\n",
    "\n",
    "# round 반올림\n",
    "\n",
    "p = 3.1415926535\n",
    "print(round(p,3))  # 소수점이하 3자리까지 반올림함\n",
    "\n",
    "\n",
    "# ceil / floor : 가까운 정수로 올려서/내려서 변환\n",
    "# ceil 가까운 정수로 올림\n",
    "# floor 가까운 정수로 올림\n",
    "# 내장함수가 아님. math를 import하여야 함\n",
    "import math\n",
    "\n",
    "print(math.ceil(p)) \n",
    "print(math.floor(p)) "
   ]
